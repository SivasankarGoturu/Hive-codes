{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "appfactory888"
		},
		"Adventureworkspace_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Adventureworkspace'"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"AzureSynapseAnalytics_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics'"
		},
		"MyDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MyDatabase'"
		},
		"VMFIleSystem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'VMFIleSystem'"
		},
		"VMFiles_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'VMFiles'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake0007.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake0007.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake0007.dfs.core.windows.net/"
		},
		"VMFIleSystem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.22.1\\nginx-1.22.1\\logs"
		},
		"VMFIleSystem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "siva"
		},
		"VMFiles_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.22.1\\nginx-1.22.1\\logs"
		},
		"VMFiles_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "siva"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "appfactory888_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Adventureworkspace_connectionString": {
						"value": "[parameters('Adventureworkspace_connectionString')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"AzureSynapseAnalytics_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics_connectionString')]"
