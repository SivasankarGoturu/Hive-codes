					"VMFIleSystem_password": {
						"value": "[parameters('VMFIleSystem_password')]"
					},
					"VMFiles_password": {
						"value": "[parameters('VMFiles_password')]"
					},
					"VMFIleSystem_properties_typeProperties_host": {
						"value": "[parameters('VMFIleSystem_properties_typeProperties_host')]"
					},
					"VMFIleSystem_properties_typeProperties_userId": {
						"value": "[parameters('VMFIleSystem_properties_typeProperties_userId')]"
					},
					"VMFiles_properties_typeProperties_host": {
						"value": "[parameters('VMFiles_properties_typeProperties_host')]"
					},
					"VMFiles_properties_typeProperties_userId": {
						"value": "[parameters('VMFiles_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appfactory888_ArmTemplate_1')]"
			]
		},
		{
			"name": "appfactory888_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appfactory888_ArmTemplate_2')]"
			]
		}
	]
